{
  "manifest_version": 2,
  "name": "More Tab Shortcuts",
  "description": "A simple extension that adds useful shortcuts to manage your tabs",
  "version": "0.7.3",
  "browser_action": {
    "default_title": "More Tab Shortcuts",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "key": "",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "node_modules/mousetrap/mousetrap.min.js",
        "content.js"
      ],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "./content.js",
    "./options.css",
    "./options.js"
  ]
}
